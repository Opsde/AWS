# To create new instance
aws ec2 run-instances --image-id ami-xxxxxxxx --count 1 --instance-type t2.micro --key-name MyKeyPair --security-group-ids sg-xxxxxxxx

# To start and stop the instance
aws ec2 describe-instances
aws ec2 describe-instances --instance-id i-xxxxxxxxxx
aws ec2 stop-instances --instance-id i-xxxxxxxxxx
aws ec2 start instances --instance-id i-xxxxxxxxxx
